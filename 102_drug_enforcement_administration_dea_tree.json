    "pubchem_description": {
      "StringWithMarkup": [
        {
          "String": "The DEA (Drug Enforcement Administration) drug and chemical classification tree contains chemical substances from the controlled substance schedules, substance classes, and regulated chemical lists. [21 eCFR-II]",
          "Markup": [
            {
              "Start": 200,
              "Length": 10,
              "URL": "https://www.ecfr.gov/current/title-21/chapter-II"
            }
          ]
        }
      ]
    },
    "pubchem_comments": {
      "StringWithMarkup": [
        {
          "String": "The controlled substances are classified into 5 categories [Controlled Substances Act (21 USC sec. 801 et seq.) and 21 eCFR 1308.11-1308.15], and the chemical control program contains List I and List II chemicals [21 eCFR Section 1310.02(a)(b)].",
